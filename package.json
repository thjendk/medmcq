{
  "name": "medmcq",
  "version": "2.0.0",
  "description": "En webapp til visning af MCQ-opgaver fra kandidaten i medicin p√• AU",
  "main": "server/server.js",
  "repository": "https://github.com/Morsby/medmcq",
  "scripts": {
    "start": "ts-node -P server/tsconfig.json -r tsconfig-paths/register server/build/server.js",
    "server": "cd server && npm run dev",
    "client": "npm run genversion && cd client && npm start",
    "dev": "cross-env npm-run-all --parallel server client",
    "install-all": "npm install && cd server && npm install && cd .. && cd client && npm install",
    "build": "cross-env npm run genversion && npm-run-all --sequential build:*",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "genversion": "genversion -e version.js -s && mv version.js client/src/",
    "test": "npm run db:reset &&  start-server-and-test testdev http://localhost:3000 cy:run",
    "testdev": "cross-env NODE_ENV=\"test\" npm run dev",
    "postbuild": "cd server && npm run migrate-latest",
    "generate": "graphql-codegen",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "db:reset": "cd server && cross-env NODE_ENV=\"test\" knex migrate:latest && cross-env NODE_ENV=\"test\" knex seed:run"
  },
  "author": "Morsby & Thomas",
  "license": "MIT",
  "dependencies": {
    "genversion": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.0",
    "@graphql-codegen/typescript": "^1.13.0",
    "@graphql-codegen/typescript-operations": "^1.13.0",
    "@graphql-codegen/typescript-react-apollo": "^1.13.0",
    "@graphql-codegen/typescript-resolvers": "^1.13.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.37",
    "@types/dotenv-flow": "^3.0.0",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.20",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^0.7.2",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.11.7",
    "@types/passport": "^1.0.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-highlighter": "^0.3.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-tagsinput": "^3.19.6",
    "@types/react-toastify": "^4.0.2",
    "@types/styled-components": "^4.4.2",
    "@types/supertest": "^2.0.8",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.11",
    "tslint": "^5.20.0",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.42.1"
  }
}
